<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Bank of America - Smoke Test" verbose="1">
    <parameter name="googleurl" value="https://www.google.com"></parameter>
    <parameter name="doiturl" value="https://www.letskodeit.com/practice"></parameter>

    <test name="google">
        <classes>
            <class name="automatedScripts.GoogleTest"/>
        </classes>
    </test>

    <test name="Different_Actions">
        <parameter name="cardropdown" value="Benz"></parameter>
        <classes>
            <class name="automatedScripts.DifferentActions"/>
        </classes>
    </test>

</suite>
